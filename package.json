{
  "name": "homesprint",
  "productName": "HomeSprint",
  "version": "1.0.0",
  "description": "A satirical project management system for household tasks",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:react\" \"npm run dev:electron\"",
    "dev:react": "vite",
    "dev:electron": "wait-on tcp:5173 && tsc -p tsconfig.main.json && electron .",
    "build": "tsc -p tsconfig.main.json && tsc -p tsconfig.renderer.json && vite build && electron-builder",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "keywords": [
    "electron",
    "react",
    "jira",
    "parody",
    "productivity"
  ],
  "author": "Antigravity",
  "license": "MIT",
